{
    "name": "starter",
    "version": "1.0.0",
    "description": "landing page for natrous",
    "main": "index.js",
    "scripts": {
        "watch:sass": "node-sass sass/main.scss css/style.css -w",
        "compile:sass": "node-sass sass/main.scss css/style.comp.css ",
        "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
        "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
        "copress:css": "node-sass css/style.prefix.css css/style.min.css --output-style compressed"
    },
    "author": "Ravi",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^10.3.1",
        "concat": "^1.0.3",
        "node-sass": "^6.0.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1"
    },
    "dependencies": {}
}
